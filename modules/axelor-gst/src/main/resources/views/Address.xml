<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="address-grid" title="Address List" model="com.axelor.gst.db.Address">
    <field name="type" title="Type" />
    <field name="line1" title="Line1"/>
    <field name="line2" title="Line2"/>
     <field name="city" title="City" />
   <field name="state" title="State"/>
    <field name="country" title="Country"/>
    <field name="pinCode" title="Pin Code"/>
  </grid>

  <form name="address-form" title="Address" cols="2" model="com.axelor.gst.db.Address">
    <panel title="Overview">
    <field name="type" selection="address.type.selection" />
    <field name="line1" />
    <field name="line2" />
    <field name="city" />
    <field name="state" />
    <field name="country" />
   	<field name="pinCode" title="Pin Code" />
    </panel>
  </form>

</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="bank-grid" title="Address List" model="com.axelor.gst.db.BankDetail">
    <field name="bank" />
    <field name="accountNo" title="Account No"/>
    <field name="ifsc" title="IFSC"/>
    <field name="type" />
   	<field name="address" />
  </grid>
	
  <form name="bank-form" title="Address" cols="2" model="com.axelor.gst.db.BankDetail">
    <panel title="Overview">
    <field name="bank" />
    <field name="accountNo" title="Account No"/>
    <field name="ifsc" title="IFSC"/>
    <field name="type" selection="bankdetail.type.selection"/>
   	<field name="address" />
    </panel>
  </form>

</object-views>
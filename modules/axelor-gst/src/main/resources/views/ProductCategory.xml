<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

  <grid name="productcat-grid" title="Product Category List" model="com.axelor.gst.db.ProductCategory">
    <field name="name" />
    <field name="code" />
    <field name="gstRate" title="GST Rate"/>
  </grid>

  <form name="productcat-form" title="Product Category" cols="2" model="com.axelor.gst.db.ProductCategory">
    <panel title="Overview">
    <field name="name" />
    <field name="code" />
    <field name="gstRate" title="GST Rate"/>
    </panel>
  </form>

</object-views>